<% if(success){ %>
    <div class="container p-3">
        <div class="row">
            <div class="container p-1">
                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                    <% success %>
                </div>
            </div>
        </div>
    </div>
<% } %>

<% if(message){ %>
    <div class="container p-3">
        <div class="row">
            <div class="container p-1">
                <div class="alert alert-danger alert-dismissible fade show" role="alert">
                    <% message %>
                </div>
            </div>
        </div>
    </div>
<% } %>